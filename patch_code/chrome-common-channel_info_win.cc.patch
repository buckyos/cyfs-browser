+  (void)SHGetSpecialFolderPath(nullptr, buffer, CSIDL_APPDATA, false);
+  base::FilePath user_data_path(buffer);
+  user_data_path = user_data_path.AppendASCII("Cyfs_Browser")
+    .AppendASCII("Cyfs_Browser")
